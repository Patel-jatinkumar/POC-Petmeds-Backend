{
    "name": "plugin_redirect",
    "version": "0.0.1",
    "description": "A plugin made for hybrid deployment, it adds page level redirects",
    "devDependencies": {
        "chai": "^3.5.0",
        "chai-subset": "^1.6.0",
        "eslint": "^3.19.0",
        "eslint-config-airbnb-base": "^5.0.3",
        "eslint-plugin-import": "^1.16.0",
        "eslint-plugin-sitegenesis": "~1.0.0",
        "proxyquire": "1.7.4",
